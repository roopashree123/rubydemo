<h1>Listing mostones</h1>

<table>
  <tr>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mostones.each do |mostone| %>
  <tr>
    <td><%= mostone.phone %></td>
    <td><%= link_to 'Show', mostone %></td>
    <td><%= link_to 'Edit', edit_mostone_path(mostone) %></td>
    <td><%= link_to 'Destroy', mostone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mostone', new_mostone_path %>
